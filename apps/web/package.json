{
  "name": "web",
  "description": "web app for recodecamp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "react",
    "vite",
    "react-router",
    "typescript"
  ],
  "scripts": {
    "dev": "vite --force",
    "build": "tsc && vite build",
    "start": "npx http-server --cors true ./dist",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier . --write",
    "typecheck": "tsc",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn tsc",
      "yarn eslint --fix",
      "yarn pretty-quick --staged"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.16",
    "@mantine/dates": "^6.0.16",
    "@mantine/dropzone": "^6.0.16",
    "@mantine/form": "^6.0.16",
    "@mantine/hooks": "^6.0.16",
    "@mantine/modals": "^6.0.16",
    "@mantine/notifications": "^6.0.16",
    "@mantine/nprogress": "^6.0.16",
    "@mantine/prism": "^6.0.16",
    "@mantine/spotlight": "^6.0.16",
    "@reduxjs/toolkit": "^1.9.5",
    "@tabler/icons-react": "^2.23.0",
    "@tanstack/react-query": "^4.29.19",
    "@tiptap/core": "^2.0.3",
    "@tiptap/extension-link": "^2.0.3",
    "@tiptap/pm": "^2.0.3",
    "@tiptap/react": "^2.0.3",
    "add": "^2.0.6",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "embla-carousel-react": "^8.0.0-rc07",
    "framer-motion": "^10.12.18",
    "http-server": "^14.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "styled-components": "^6.0.3",
    "vite-plugin-svgr": "^3.2.0"
  },
  "devDependencies": {
    "@redux-devtools/extension": "^3.2.5",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "https": "^1.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.24",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-cross-origin-isolation": "^0.1.6",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
